新規投稿画面
    <%= form_with model: @game_post do |f| %>
              <div class="input-group mb-3 mt-4">
            <h4>投稿タイトル</h4>
            <%= f.text_field :post_title, class: "form-control", placeholder: "〇〇で遊んだ！" %>
            <h4>投稿内容</h4>
            <%= f.text_area :post_body, class: "form-control", placeholder: "1位取れた！" %>
            <h4>ショップ名</h4>
            <%= f.text_field :shop_name, class: "form-control", placeholder: "買った・遊んだショップ" %>
            <h4>住所</h4>
            <%= f.text_field :shop_address, class: "form-control", placeholder: "ショップの住所" %>
          </div>


        <%= f.submit '投稿', class: "btn btn-lg btn-block mt-4" %>
  <% end %>